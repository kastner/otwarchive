<h1>New translation_note</h1>

<% form_for(@translation_note) do |f| %>
  <%= f.error_messages %>

  <p>
    <%= f.label :namespace %><br />
    <p><%= f.select :namespace, @namespaces, :selected => @current_namespace %></p>
  </p>
  <p>
    <%= f.label :note, 'Your notes:' %><br />
    <%= f.text_area :note %>
  </p>
  <p>
    <%= f.submit "Create" %>
  </p>
<% end %>

<%= link_to 'Back', translations_path %>
