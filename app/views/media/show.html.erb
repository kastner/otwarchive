<h1><%= link_to 'Fandoms', media_path %> > <%=h @medium.name %></h1>
<ul>
  <%- for fandom in @fandoms -%>
    <li><%= print_fandom_works_link(fandom) %></li>
  <%- end -%>
</ul>

<div class="pagination">
	<%= will_paginate @fandoms, :prev_label =>  "&laquo; Previous".t,:next_label => "Next &raquo;".t %>
</div>