<h1><%=h "View All Series" %></h1>

<ul class="work-list">
  <% for series in @series %>
	<%= render :partial => 'series/series_blurb', :locals => {:series => series} %>
  <% end %>
</ul>

<div class="pagination">
	  <%= will_paginate @series, :prev_label =>  "&laquo; Previous",:next_label => "Next &raquo;" %>
</div>
