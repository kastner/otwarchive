<h1>All Users:</h1>

<table id="admin_users_table">
<caption>List of all current users in the archive</caption>
<tr>
	<th scope="col">Username</th>
	<th scope="col">Email</th>
	<th scope="col">Translator</th>
	<th scope="col">Tag Wrangler</th>
	<th scope="col">Suspended</th>
	<th scope="col">Banned</th>
	<th scope="col">Update</th>
</tr>
	<%- for user in @users -%>
	<tr id="user_<%= user.id %>">
    <%= render :partial => 'user_form', :locals => {:user => user} %>
  </tr> 
<%- end -%>
</table>

<div class="pagination">
	  <%= will_paginate @users, :prev_label =>  "&laquo; Previous".t,:next_label => "Next &raquo;".t %>
</div>